{
  "preset": "folder-based",
  "depth": 3,
  "logFile": ".commit-logs/violations.log",
  "enabled": true,
  "ignorePaths": [
    "package.json",
    "package-lock.json",
    "tsconfig.json",
    ".gitignore",
    "README.md",
    ".precommitrc.json",
    "CHANGELOG.md",
    "USAGE.md",
    ".husky"
  ],
  "maxFiles": 100,
  "verbose": false,
  "language": "en"
}
