{
  "depth": 2,
  "logFile": ".commit-logs/violations.log",
  "enabled": true,
  "ignorePaths": [
    "package.json",
    "package-lock.json",
    "tsconfig.json",
    ".gitignore",
    "README.md",
    ".precommitrc.json",
    "CHANGELOG.md",
    "USAGE.md",
    ".husky",
    "src"
  ],
  "maxFiles": 100,
  "verbose": false,
  "logMaxAgeHours": 24,
  "autoCleanupLogs": true
}
