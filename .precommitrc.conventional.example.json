{
  "preset": "conventional-commits",
  "logFile": ".commit-logs/violations.log",
  "enabled": true,
  "verbose": false,
  "language": "en",
  "types": ["feat", "fix", "docs", "style", "refactor", "perf", "test", "build", "ci", "chore", "revert"],
  "scopes": ["api", "ui", "db", "auth", "core"],
  "requireScope": false
}
